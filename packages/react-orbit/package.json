{
  "name": "react-orbit-component",
  "version": "0.0.0",
  "author": "Uhmpasterig",
  "description": "A Orbit component for React.",
  "license": "MIT",
  "homepage": "https://test.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/uhmpasterig/react-orbit-component"
  },
  "bugs": {
    "url": "https://github.com/uhmpasterig/react-orbit-component/issues"
  },
  "private": false,
  "keywords": [
    "react",
    "orbit",
    "component"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup --dts --minify",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component"
  },
  "devDependencies": {
    "@turbo/gen": "^1.11.3",
    "@types/eslint": "^8.56.1",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "tsup": "^8.0.2"
  }
}
